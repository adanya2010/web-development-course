class Human:
    eyes=2
    nose=1
    mouth=1
    ears=2
    hands =2
    legs=2

class Animal:
    legs=4
    tail=1
    eyes=2
    ears=2
    sound="generic"
    
dog=Animal()
cat=Animal()
cow=Animal()

#mobiles
class mobiles:
    brand="iphone"
    ram=6
    rom=128
    battery=5000
    camera=48
    
samsung=mobiles()
iphone=mobiles()
oneplus=mobiles()

print(samsung.ram)
print(iphone.camera)
print(oneplus.battery)

class student :
    def_init_(self,n,c,r):
        self.Name=n
        self.rollno.=r
        self.class=c
    student1=("adanya",1)
    student2=("ananya",45)
    student3=("jackie",2)
print(student1.name)
print(student2.name)
