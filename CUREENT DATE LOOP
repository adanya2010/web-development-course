<!DOCTYPE html>
<html>
<head>
    <title>Random Numbers & Date</title>
</head>
<body>
    <h2>Random Numbers with Current Date</h2>
    <button onclick="generateNumbers()">Generate</button>
    <ul id="numbersList"></ul>

    <script>
        function generateNumbers() {
            const list = document.getElementById("numbersList");
            list.innerHTML = ""; // Clear previous results

            const date = new Date(); // Get current date
            const formattedDate = date.toDateString() + " " + date.toLocaleTimeString();

            // Show current date
            const dateItem = document.createElement("li");
            dateItem.textContent = "Current Date & Time: " + formattedDate;
            list.appendChild(dateItem);

            // Loop to generate 5 random numbers
            for (let i = 1; i <= 5; i++) {
                let randomNumber = Math.floor(Math.random() * 100) + 1; // 1 to 100
                let listItem = document.createElement("li");
                listItem.textContent = "Random Number " + i + ": " + randomNumber;
                list.appendChild(listItem);
            }
        }
    </script>
</body>
</html>
